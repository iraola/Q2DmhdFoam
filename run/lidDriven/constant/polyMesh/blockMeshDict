            (8 12 15 11)
        );
    }
    outlet
    {
        type patch;
        faces
        (
            (2 6 5 1)
            (9 13 6 2)
            (10 14 13 9)
            (17 19 14 10)
        );
    }
    lowerWall
    {
        type patch;
        faces
        (
            (1 5 4 0)
        );
    }
    upperWall
    {
        type patch;
        faces
        (
            (18 19 17 16)
        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (0 3 2 1)
            (3 8 9 2)
            (8 11 10 9)
            (11 16 17 10)
            (4 5 6 7)
            (7 6 13 12)
