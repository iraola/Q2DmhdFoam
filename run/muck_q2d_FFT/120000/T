FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "120000";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    xinlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    xoutlet
    {
        type            zeroGradient;
    }
    sideWalls
    {
        type            zeroGradient;
    }
    hartmannWalls
    {
        type            empty;
    }
    cylinder
    {
        type            zeroGradient;
    }
}


