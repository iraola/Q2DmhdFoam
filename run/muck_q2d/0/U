FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.00021866 0.00000002 0);

boundaryField
{
    xinlet
    {
        type            fixedValue;
        value           uniform (0.00021866 0 0);
    }
    xoutlet
    {
        type            advective;
        //type            inletOutlet;
        //value           uniform (0.00021866 0.0 0);
        //inletValue           uniform (0 0 0);
	inletOutlet	    true;
    	correctSupercritical false;
    }
    sideWalls
    {
        type            fixedValue;
    	value		    uniform (0 0 0);
    }
    hartmannWalls
    {
        type            empty;
    }
    cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


