FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform 0.0037070;    //  0.0016525);

boundaryField
{
    xinlet
    {
        type            zeroGradient;
    }
    xoutlet
    {
        type            zeroGradient;
    }
    sideWalls
    {
        type            zeroGradient;
    }
    hartmannWalls
    {
        type            zeroGradient;
    }
    cylinder
    {
        type            zeroGradient;
    }
}


