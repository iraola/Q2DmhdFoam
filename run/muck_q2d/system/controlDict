FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libfieldFunctionObjects.so" "libforces.so");

application 	Q2DmhdFoam; 

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         500000;

deltaT          10;

writeControl    adjustableRunTime;

writeInterval   5000; 

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.2;

epsSteady	1e-08;

ssCriteria      false;

functions
{
	probes
	{
	    type            probes;
	    libs            ("libsampling.so");

	    writeControl    timeStep;
	    writeInterval   10;
	    fields
	    (
		U
	    );

	    // Locations to be probed. runTime modifiable!
	    probeLocations
	    (
		(1.24 1 0)
		(1.34 1 0)
		(1.44 1 0)
	    );
	}

	
	forcesCo
	{
	    type            forceCoeffs;
	    functionObjectlibs        ( "libforces.so" );
	    outputControl    timeStep;
	    timeInterval    10;
	    patches         ( "c.*" );    
	    rhoInf          9838.6622;   // Redundant for incompressible
	    rhoName         rhoInf;      // Indicates incompressible
	    pName           p;
	    UName           U;
	    log             true;
	    liftDir         (0 1 0);
	    dragDir         (1 0 0);
	    CofR            (1.1 1 1);   // Axle midpoint on ground
	    pitchAxis       (0 1 0);
	    magUInf         0.00021866;
	    lRef            0.2;         // Wheelbase length
	    Aref            0.004;         // Estimated   (2*b)*w=  (0.02)*0.2=  0.004 
	}
}	


	// ************************************************************************* //
