// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application Q2DmhdFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e6;

deltaT          0.005;

writeControl    adjustableRunTime;

writeInterval   100;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

epsSteady       1e-7;

maxCo           0.5;


